@model IEnumerable<Slot>

<h2>Rooms</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(x => x.RoomID)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            @if ()
            {
            <tr>
                <td>@Html.DisplayFor(x => item.RoomID)</td>
            </tr>
            }
        }
    </tbody>
</table>